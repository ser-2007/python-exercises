            "source": [
                "def power_set(s):\r\n",
                "  result = []\r\n",
                "  for i in s:\r\n",
                "    result += [j +[i] for j in result]\r\n",
                "    result.append([i]) \r\n",
                "  result.append([])\r\n",
                "  return sorted(result, key =len)"
            ],
